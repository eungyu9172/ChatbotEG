   "source": [
    "import os\n",
    "\n",
    "\n",
    "def iter_pdf_paths(root: str):\n",
    "    for dirpath, _, filenames in os.walk(root):\n",
    "        for f in filenames:\n",
    "            if f.lower().endswith(\".pdf\"):\n",
    "                yield os.path.join(dirpath, f)"
   ]
  },
